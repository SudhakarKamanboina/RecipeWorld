<script type="text/javascript">
    var temp =' <%= query %>';
    //console.log(temp);

    $.ajax({
        method: "GET",
        url: "http://api.yummly.com/v1/api/recipes?_app_id=82eccb5e&_app_key=0c0f7b0c0d1cde681c221f41216ea5b5&q="+temp,
        cache: false,
        success: function(res) {
            console.log(res);
            var recipes = res.matches;
            var url;
            for(i=0; i<recipes.length; i++) {
                    url = recipes[i].imageUrlsBySize[90];
                    url = url.substr(0, url.length - 5);
                    $('#img' + i).attr("src", url + "s430-c");
                    $('#imgref' + i).attr("href", "/recipe/"+recipes[i].id);
                    $('#recipe' + i).html(recipes[i].recipeName);
                    //$('#desc' + i).html("Cuisine: "+recipes[i].attributes.cuisine[0]);
            }

        },
        error: function(err) {
            console.log(err);
        }
    });


</script>
